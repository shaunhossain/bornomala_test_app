<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NumberActivity">

    <androidx.cardview.widget.CardView
        android:id="@+id/one"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:innerRadius="0dp"
        android:shape="oval"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toTopOf="@+id/two"
        app:layout_constraintEnd_toStartOf="@+id/six"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/one_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/view_color_related"
            app:srcCompat="@drawable/one" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/two"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:innerRadius="0dp"
        android:shape="oval"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toTopOf="@+id/three"
        app:layout_constraintEnd_toStartOf="@+id/seven"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/one">

        <ImageView
            android:id="@+id/two_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/view_color_related"
            app:srcCompat="@drawable/two" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/three"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:innerRadius="0dp"
        android:shape="oval"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toTopOf="@+id/four"
        app:layout_constraintEnd_toStartOf="@+id/eight"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/two">

        <ImageView
            android:id="@+id/three_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/view_color_related"
            app:srcCompat="@drawable/three" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/four"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:innerRadius="0dp"
        android:shape="oval"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toTopOf="@+id/five"
        app:layout_constraintEnd_toStartOf="@+id/nine"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/three">

        <ImageView
            android:id="@+id/four_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/view_color_related"
            app:srcCompat="@drawable/four" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/five"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:innerRadius="0dp"
        android:shape="oval"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/zero"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/four">

        <ImageView
            android:id="@+id/five_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/view_color_related"
            app:srcCompat="@drawable/five" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/nine"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:innerRadius="0dp"
        android:shape="oval"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toTopOf="@+id/zero"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/four"
        app:layout_constraintTop_toBottomOf="@+id/eight" >

        <ImageView
            android:id="@+id/nine_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/view_color_related"
            app:srcCompat="@drawable/nine" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/six"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:innerRadius="0dp"
        android:shape="oval"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toTopOf="@+id/seven"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/one"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/six_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/view_color_related"
            app:srcCompat="@drawable/six" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/seven"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:innerRadius="0dp"
        android:shape="oval"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toTopOf="@+id/eight"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/two"
        app:layout_constraintTop_toBottomOf="@+id/six">

        <ImageView
            android:id="@+id/seven_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/view_color_related"
            app:srcCompat="@drawable/seven" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/eight"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:innerRadius="0dp"
        android:shape="oval"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toTopOf="@+id/nine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/three"
        app:layout_constraintTop_toBottomOf="@+id/seven">

        <ImageView
            android:id="@+id/eight_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/view_color_related"
            app:srcCompat="@drawable/eight" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/zero"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:innerRadius="0dp"
        android:shape="oval"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/five"
        app:layout_constraintTop_toBottomOf="@+id/nine">

        <ImageView
            android:id="@+id/zero_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/view_color_related"
            app:srcCompat="@drawable/zero" />
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>